    "axios": "^1.11.0",
    "codemirror": "^6.0.2",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "element-plus": "^2.10.4",
    "jsencrypt": "^3.3.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "qs": "^6.14.0",
    "sortablejs": "^1.15.6",
    "tinymce": "^7.9.1",
    "tree-lodash": "^0.4.0",
    "ts-md5": "^2.0.1",
    "version-rocket": "^1.7.4",
    "vue": "^3.5.18",
    "vue-command-palette": "^0.2.3",
